{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Flask (run app.py)",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/app.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"env": {
				"FLASK_ENV": "development"
			},
			"envFile": "${workspaceFolder}/.env",
			"justMyCode": true
		},
	]
}

